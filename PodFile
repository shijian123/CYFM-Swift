platform :ios, '10.0'
install! 'cocoapods',
generate_multiple_pod_projects: true

target 'CYFM-Swift' do

use_frameworks!
# 忽略引入库的所有警告
inhibit_all_warnings!

# Pods for LBFM-Swift
pod 'SnapKit'
pod 'Kingfisher'
#tabbar样式
pod 'ESTabBarController-swift', '~> 2.6.2'
#banner滚动图片
pod 'FSPagerView'
#网络请求
pod 'Moya'
pod 'HandyJSON', '~> 5.0.0-beta.1'
pod 'SwiftyJSON'
# 分页
pod 'DNSPageView', '~> 1.2.1'
#跑马灯
pod 'JXMarqueeView'
#滚动页
pod 'LTScrollView'
pod 'MJRefresh'
#pod 'SkeletonView'
#消息提示
pod 'SwiftMessages'
#pod 'YYKit'
pod 'SVProgressHUD'
#播放网络音频
pod 'StreamingKit'

end

